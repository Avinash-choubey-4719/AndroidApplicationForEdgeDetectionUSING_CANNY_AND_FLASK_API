<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/uploadedImage"
        android:layout_width="317dp"
        android:layout_height="212dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.393"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon"
        tools:srcCompat="@drawable/icon" />

    <ImageView
        android:id="@+id/resultantImage"
        android:layout_width="312dp"
        android:layout_height="238dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.424"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/uploadedImage"
        app:layout_constraintVertical_bias="0.628"
        app:srcCompat="@drawable/icon"
        tools:srcCompat="@drawable/icon" />

    <Button
        android:id="@+id/send"
        android:layout_width="143dp"
        android:layout_height="48dp"
        android:text="SendToExtract"
        app:layout_constraintBottom_toTopOf="@+id/resultantImage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.156"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/uploadedImage"
        app:layout_constraintVertical_bias="0.466" />

    <Button
        android:id="@+id/upload"
        android:layout_width="143dp"
        android:layout_height="48dp"
        android:text="UploadImage"
        app:layout_constraintBottom_toTopOf="@+id/resultantImage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toEndOf="@+id/send"
        app:layout_constraintTop_toBottomOf="@+id/uploadedImage"
        app:layout_constraintVertical_bias="0.466" />

    <Button
        android:id="@+id/save"
        android:layout_width="143dp"
        android:layout_height="48dp"
        android:text="SaveImage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resultantImage" />

</androidx.constraintlayout.widget.ConstraintLayout>